OPENAI_BASE_URL=http://127.0.0.1:5001/v1
OPENAI_API_KEY=fake_key

OVERRIDE_MODEL=gpt-4

RESPOND_WITH_CACHE=true
STORE_CACHE=true
CACHE_DIR=./cache

# Mock Response Feature
# Set to true to return mock responses instead of calling actual API
MOCK_RESPONSE=false
# Simulated delay in seconds for mock responses (0.5 = 500ms)
MOCK_DELAY=0.5

# Customizable Mock Content
# Chat completion mock response (supports placeholders: {user_message}, {model}, {request_id})
MOCK_CHAT_CONTENT="[MOCK] Hello! This is a simulated response to your message: '{user_message}' using model {model}."
# Text completion mock response (supports placeholders: {prompt}, {model}, {request_id})
MOCK_COMPLETION_CONTENT="[MOCK] This is a simulated completion for prompt: '{prompt}' using {model}."
# Available models in mock mode (comma-separated)
MOCK_MODELS="gpt-4,gpt-3.5-turbo,claude-3-sonnet,text-davinci-003,custom-model-v1"
# Token usage for mock responses: prompt_tokens,completion_tokens,total_tokens
MOCK_TOKEN_USAGE="75,35,110"

TIMEOUT=3000.0
PORT=5001